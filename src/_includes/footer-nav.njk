{# <nav class="navigation"> #}
<nav>
  <ul
    class="navigation-menu-footer">
    {# <ul class=""> #}
    {% for item in navigation.footer %}
      {%- set url = item.url | locale_url %}
      <li class="">
        <a href="{{ url }}" {% if page.url === url %} aria-current="page" {% endif %}>
          {{ item.text }}
        </a>
      </li>
    {%- endfor %}
  </ul>
</nav>